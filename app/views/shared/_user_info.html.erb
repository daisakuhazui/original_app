  <a href="<%= user_path(current_user) %>" class="user_profile_image">
    <%= image_tag user.profile_image, size: 70 %>
  </a>
  <div class="user_name">
    <h1>
      <%= current_user.name %>
    </h1>
    <div>
      @ <%= user.alias_name %>
    </div>
  </div>
  <span>
    <%= link_to "view my profile", current_user %>
  </span>
